        "yaml": "^2.3.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/react": "^19.2.10",
        "@vitest/coverage-v8": "^1.6.1",
        "tsup": "^8.0.0",
        "tsx": "^4.0.0",
        "typescript": "^5.3.0",
        "vitest": "^1.0.0"
    },
    "engines": {
        "node": ">=18.0.0"
