{
    "name": "skillwisp",
    "version": "0.6.4",
    "description": "Developer tool integrations installer",
    "keywords": [
        "skillwisp",
        "ai",
        "agent",
        "skills",
        "cli"
    ],
    "homepage": "https://gitcode.com/norix77/skillwisp-cli",
    "repository": {
        "type": "git",
        "url": "https://gitcode.com/norix77/skillwisp-cli"
    },
    "license": "MIT",
    "author": "SkillWisp Team",
    "type": "module",
    "bin": {
        "skillwisp": "./dist/index.js"
    },
